<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>mach.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>rebord</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <factor-prop>sim/model//material/instruments/factor</factor-prop>
    </emission>
  </animation>

  <!-- Mach -->
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>velocities/mach</property>
    <interpolation>
     <entry><ind> 0.0 </ind><dep>   0 </dep></entry>
     <entry><ind> 1.0 </ind><dep>  41 </dep></entry>
     <entry><ind> 1.5 </ind><dep> 111 </dep></entry>
     <entry><ind> 2.0 </ind><dep> 186 </dep></entry>
     <entry><ind> 2.5 </ind><dep> 262 </dep></entry>
     <entry><ind> 3.0 </ind><dep> 340 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
